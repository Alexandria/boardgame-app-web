{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "interface-over-type-literal": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "prettier": [
      true,
      {
        "printWidth": 91,
        "semi": false,
        "trailingComma": "es5"
      }
    ],
    "trailing-comma": [
      true,
      "spec"
    ]
  },
  "rulesDirectory": []
}